package day15_Scanner_StringClass;

public class Task01 {

	public static void main(String[] args) {
		
		
   int z = 10;
   switch(z) {
   case 10: System.out.println("Monday");
   case11: System.out.println("Tuesday");
   
   default: System.out.println("Friday");
   }
		
		
		
		
		
		
		
		

	}

}
